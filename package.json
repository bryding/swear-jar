{
  "name": "swear-jar",
  "version": "1.0.0",
  "description": "Shared swear jar tracking app for Ben and Kaiti",
  "main": "server/server.js",
  "scripts": {
    "start": "cd server && npm install && npm start",
    "dev": "cd server && npm run dev",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "install:all": "npm install && cd server && npm install"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "express": "^5.1.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom": "^22.1.0",
    "jsdom-global": "^3.0.2",
    "redis-memory-server": "^0.9.0",
    "supertest": "^6.3.4"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "keywords": [
    "swear-jar",
    "tracking",
    "family"
  ],
  "author": "Ben",
  "license": "MIT"
}
